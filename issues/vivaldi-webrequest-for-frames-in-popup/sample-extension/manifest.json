{
  "manifest_version": 2,
  "name": "Vivaldi WebRequest Demo",
  "version": "3.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "browser_style": false,
    "default_popup": "/popup.html"
  },
  "background": {
    "scripts": ["/webrequest.js"],
    "persistent": true
  }
}
