{
  "manifest_version": 2,
  "name": "vivaldi-82634-webrequest-for-frames-in-popup",
  "description": "vivaldi-82634-webrequest-for-frames-in-popup",
  "version": "4.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "browser_style": false,
    "default_popup": "/popup.html"
  },
  "icons": {
    "16": "/icon.png"
  },
  "background": {
    "scripts": ["/webrequest.js"],
    "persistent": true
  }
}
