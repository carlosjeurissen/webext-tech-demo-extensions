{
  "manifest_version": 3,
  "name": "messaging-sub-frame-cs-to-extension-top-frame",
  "description": "messaging-sub-frame-cs-to-extension-top-frame",
  "version": "1.0",
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [{
    "all_frames": true,
    "matches": ["<all_urls>"],
    "js": ["/content-script.js"]
  }],
  "icons": {
    "16": "/icon.png",
    "48": "/icon.png"
  },
  "background": {
    "scripts": ["/background.js"],
    "service_worker": "/background.js"
  },
  "action": {
    "default_area": "navbar"
  }
}
