{
  "manifest_version": 3,
  "name": "content-scripts-exclude-matches",
  "description": "content-scripts-exclude-matches",
  "version": "1.0",
  "icons": {
    "16": "/icon.png"
  },
  "host_permissions": ["https://*.example.com/*"],
  "permissions": ["scripting"],
  "background": {
    "service_worker": "/background.js",
    "scripts": ["/background.js"]
  }
}
