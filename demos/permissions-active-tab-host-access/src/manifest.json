{
  "manifest_version": 3,
  "name": "permissions-active-tab",
  "description": "permissions-active-tab",
  "version": "0.0.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "service_worker": "background.js"
  },
  "action": {
    "default_area": "navbar",
    "default_popup": "/popup.html"
  }
}
