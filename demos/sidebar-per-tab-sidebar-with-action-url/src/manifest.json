{
  "manifest_version": 3,
  "name": "Multiple side panels",
  "description": "side panels",
  "version": "1.0",
  "icons": {
    "16": "/icon.png"
  },
  "background": {
    "scripts": ["/background.js"],
    "service_worker": "/background.js"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "permissions": [
    "sidePanel",
    "activeTab"
  ]
}
